<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CompasseRuler.Themes">
    <Color x:Key="Theme.Color.Foreground">#FF050505</Color>
    <Color x:Key="Theme.Color.Background">#FFF5F5F5</Color>

    <SolidColorBrush x:Key="Theme.Brush.Foreground" Color="{StaticResource Theme.Color.Foreground}"></SolidColorBrush>
    <SolidColorBrush x:Key="Theme.Brush.Background" Color="{StaticResource Theme.Color.Background}"></SolidColorBrush>
</ResourceDictionary>